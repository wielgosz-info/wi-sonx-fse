{
	"name": "wielgosz-info/wi-sonx-fse",
	"description": "WI Sonx - Personal CV, Resume & Portfolio WP Theme",
	"version": "1.0.0",
	"type": "wordpress-theme",
	"license": "MIT",
	"authors": [
		{
			"name": "Urszula Wielgosz"
		}
	],
	"repositories": [
        {
            "type": "git",
            "url":  "https://github.com/wielgosz-info/vite-for-wp.git"
        }
    ],
	"require": {
		"kucrut/vite-for-wp": "dev-enhancement-refresh-runtime"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^3.0"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"autoload": {
		"classmap": ["includes"]
	}
}
