{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Dark Mode",
	"settings": {
		"useRootPaddingAwareAlignments": true,
		"border": {
			"color": false,
			"radius": false,
			"width": false,
			"style": false
		},
		"shadow": {
			"defaultPresets": false,
			"presets": []
		},
		"color": {
			"background": false,
			"text": false,
			"heading": false,
			"link": false,
			"button": false,
			"caption": false,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "var(--wp--preset--color--black)",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "var(--wp--custom--color--gray-200)",
					"name": "Base / Two",
					"slug": "base-2"
				},
				{
					"color": "var(--wp--preset--color--white)",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "var(--wp--custom--color--gray-800)",
					"name": "Contrast / Two",
					"slug": "contrast-2"
				},
				{
					"color": "var(--wp--custom--color--gray-500)",
					"name": "Contrast / Three",
					"slug": "contrast-3"
				},
				{
					"color": "var(--wp--custom--color--green-700)",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "var(--wp--custom--color--green-500)",
					"name": "Accent / Two",
					"slug": "accent-2"
				},
				{
					"color": "var(--wp--custom--color--green-300)",
					"name": "Accent / Three",
					"slug": "accent-3"
				},
				{
					"color": "var(--wp--custom--color--yellow-700)",
					"name": "Accent / Four",
					"slug": "accent-4"
				},
				{
					"color": "var(--wp--custom--color--violet-700)",
					"name": "Accent / Five",
					"slug": "accent-5"
				}
			]
		},
		"background": {
			"backgroundImage": false,
			"backgroundSize": false
		},
		"dimensions": {
			"aspectRatio": false,
			"minHeight": false
		},
		"layout": {
			"contentSize": "960px",
			"wideSize": "1164px",
			"allowEditing": false,
			"allowCustomContentAndWideSize": false
		},
		"lightbox": {
			"enabled": false,
			"allowEditing": false
		},
		"position": {
			"sticky": true
		},
		"spacing": {
			"blockGap": false,
			"margin": false,
			"padding": false,
			"units": ["rem", "px", "em", "%", "vw", "vh"],
			"customSpacingSize": false,
			"spacingScale": {
				"operator": "*",
				"increment": 1.618,
				"steps": 9,
				"mediumStep": 1.618,
				"unit": "rem"
			}
		},
		"typography": {
			"customFontSize": false,
			"fontStyle": false,
			"fontWeight": false,
			"fluid": {
				"minFontSize": "0.625rem",
				"maxViewportWidth": "960px",
				"minViewportWidth": "320px"
			},
			"letterSpacing": false,
			"lineHeight": false,
			"textColumns": false,
			"textDecoration": false,
			"writingMode": false,
			"textTransform": false,
			"dropCap": false,
			"fontSizes": [
				{
					"name": "X-Small",
					"slug": "x-small",
					"size": "0.875rem"
				},
				{
					"name": "Small",
					"slug": "small",
					"size": "1rem"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "1.125rem"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "1.5rem"
				},
				{
					"name": "X-Large",
					"slug": "x-large",
					"size": "2rem"
				},
				{
					"name": "XX-Large",
					"slug": "xx-large",
					"size": "4.5rem"
				},
				{
					"name": "Huge",
					"slug": "Huge",
					"size": "9rem",
					"fluid": {
						"min": "3.5rem"
					}
				}
			],
			"fontFamilies": [
				{
					"name": "accent",
					"slug": "accent",
					"fontFamily": "Poppins, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["local('Poppins-Regular')", "file:./assets/fonts/poppins-v20-latin_latin-ext-regular.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": ["local('Poppins-Italic')", "file:./assets/fonts/poppins-v20-latin_latin-ext-italic.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": ["local('Poppins-SemiBold')", "file:./assets/fonts/poppins-v21-latin_latin-ext-600.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["local('Poppins-Bold')", "file:./assets/fonts/poppins-v20-latin_latin-ext-700.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": ["local('Poppins-BoldItalic')", "file:./assets/fonts/poppins-v20-latin_latin-ext-700italic.woff2"]
						}
					]
				}
			]
		},
		"blocks": {},
		"custom": {
			"color": {
				"green-300": "oklch(35% 0.05 183)",
				"green-500": "oklch(55% 0.075 183)",
				"green-700": "oklch(75% 0.1 183)",
				"green-900": "oklch(95% 0.1 183)",
				"gray-100": "oklch(15% 0 0)",
				"gray-200": "oklch(20% 0 0)",
				"gray-300": "oklch(30% 0 0)",
				"gray-400": "oklch(40% 0 0)",
				"gray-500": "oklch(45% 0 0)",
				"gray-600": "oklch(55% 0 0)",
				"gray-700": "oklch(70% 0 0)",
				"gray-800": "oklch(80% 0 0)",
				"gray-900": "oklch(97.5% 0 0)",
				"yellow-700": "oklch(75% 0.125 83)",
				"violet-700": "oklch(75% 0.125 283)"
			},
			"transition": {
				"duration": "0.1s",
				"timing-function": "ease"
			},
			"front-page": {
				"mobile-header-height": "5rem",
				"full-header-width": "8rem"
			}
		}
	},
	"styles": {
		"border": {
			"color": "var(--wp--preset--color--contrast-3)"
		},
		"spacing": {
			"blockGap": "0rem",
			"padding": {
				"top": "0",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--30)",
				"right": "var(--wp--preset--spacing--30)"
			}
		},
		"color": {
			"text": "var(--wp--preset--color--contrast)",
			"background": "var(--wp--preset--color--base)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--accent)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400",
			"lineHeight": "1.2"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"button": {
				"typography": {
					"fontWeight": "700",
					"textTransform": "uppercase",
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "1.4",
					"letterSpacing": "0.05em"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)",
					"background": "var(--wp--preset--color--accent)"
				},
				"border": {
					"radius": "0",
					"width": "var(--wp--preset--spacing--10)",
					"style": "solid",
					"color": "transparent"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--40)",
						"bottom": "var(--wp--preset--spacing--40)",
						"left": "var(--wp--preset--spacing--60)",
						"right": "var(--wp--preset--spacing--60)"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--accent-2)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--accent-2)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--accent-2)"
					}
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "600"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"fontWeight": "600"
				}
			},
			"caption": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--20)",
						"bottom": "var(--wp--preset--spacing--20)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"textTransform": "uppercase"
				}
			},
			"cite": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--20)",
						"bottom": "var(--wp--preset--spacing--20)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"textTransform": "uppercase",
					"fontStyle": "normal"
				}
			}
		}
	},
	"customTemplates": [],
	"templateParts": [
		{
			"name": "header-default",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "header-front-page",
			"title": "Header on Front Page",
			"area": "header"
		},
		{
			"name": "footer-default",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "loop-default",
			"title": "Posts Loop",
			"area": "loop"
		}
	],
	"patterns": []
}
