{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Dark Mode",
	"settings": {
		"useRootPaddingAwareAlignments": true,
		"border": {
			"color": false,
			"radius": false,
			"width": false,
			"style": false
		},
		"shadow": {
			"defaultPresets": false,
			"presets": []
		},
		"color": {
			"background": false,
			"text": false,
			"heading": false,
			"link": false,
			"button": false,
			"caption": false,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"name": "Base",
					"slug": "base",
					"color": "var(--wp--preset--color--black)"
				},
				{
					"name": "Contrast",
					"slug": "contrast",
					"color": "var(--wp--preset--color--white)"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "var(--wp--custom--color--green-700)"
				},
				{
					"name": "Border",
					"slug": "border",
					"color": "var(--wp--custom--color--gray-500)"
				},
				{
					"name": "Off-Base",
					"slug": "off-base",
					"color": "var(--wp--custom--color--gray-200)"
				},
				{
					"name": "Off-Contrast",
					"slug": "off-contrast",
					"color": "var(--wp--custom--color--gray-800)"
				},
				{
					"name": "Dim-Primary",
					"slug": "dim-primary",
					"color": "var(--wp--custom--color--green-500)"
				}
			]
		},
		"background": {
			"backgroundImage": false,
			"backgroundSize": false
		},
		"dimensions": {
			"aspectRatio": false,
			"minHeight": false
		},
		"layout": {
			"contentSize": "960px",
			"wideSize": "1164px",
			"allowEditing": false,
			"allowCustomContentAndWideSize": false
		},
		"lightbox": {
			"enabled": false,
			"allowEditing": false
		},
		"position": {
			"sticky": true
		},
		"spacing": {
			"blockGap": false,
			"margin": false,
			"padding": false,
			"units": ["rem", "px", "em", "%", "vw", "vh"],
			"customSpacingSize": false,
			"spacingScale": {
				"operator": "*",
				"increment": 1.618,
				"steps": 9,
				"mediumStep": 1.618,
				"unit": "rem"
			}
		},
		"typography": {
			"customFontSize": false,
			"fontStyle": false,
			"fontWeight": false,
			"fluid": {
				"minFontSize": "0.625rem",
				"maxViewportWidth": "960px",
				"minViewportWidth": "320px"
			},
			"letterSpacing": false,
			"lineHeight": false,
			"textColumns": false,
			"textDecoration": false,
			"writingMode": false,
			"textTransform": false,
			"dropCap": false,
			"fontSizes": [
				{
					"name": "X-Small",
					"slug": "x-small",
					"size": "0.875rem"
				},
				{
					"name": "Small",
					"slug": "small",
					"size": "1rem"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "1.125rem"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "1.5rem"
				},
				{
					"name": "X-Large",
					"slug": "x-large",
					"size": "2rem"
				},
				{
					"name": "XX-Large",
					"slug": "xx-large",
					"size": "4.5rem"
				},
				{
					"name": "Huge",
					"slug": "Huge",
					"size": "9rem",
					"fluid": {
						"min": "3.5rem"
					}
				}
			],
			"fontFamilies": [
				{
					"name": "Primary",
					"slug": "primary",
					"fontFamily": "Poppins, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["local('Poppins-Regular')", "file:./assets/fonts/poppins-v20-latin_latin-ext-regular.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": ["local('Poppins-Italic')", "file:./assets/fonts/poppins-v20-latin_latin-ext-italic.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["local('Poppins-Bold')", "file:./assets/fonts/poppins-v20-latin_latin-ext-700.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": ["local('Poppins-BoldItalic')", "file:./assets/fonts/poppins-v20-latin_latin-ext-700italic.woff2"]
						}
					]
				}
			]
		},
		"blocks": {},
		"custom": {
			"color": {
				"green-300": "oklch(35% 0.05 183)",
				"green-500": "oklch(55% 0.075 183)",
				"green-700": "oklch(75% 0.1 183)",
				"gray-100": "oklch(15% 0 0)",
				"gray-200": "oklch(20% 0 0)",
				"gray-300": "oklch(30% 0 0)",
				"gray-400": "oklch(40% 0 0)",
				"gray-500": "oklch(45% 0 0)",
				"gray-600": "oklch(55% 0 0)",
				"gray-700": "oklch(70% 0 0)",
				"gray-800": "oklch(80% 0 0)",
				"gray-900": "oklch(97.5% 0 0)"
			},
			"front-page": {
				"mobile-header-height": "5rem",
				"full-header-width": "8rem"
			}
		}
	},
	"styles": {
		"border": {
			"color": "var(--wp--preset--color--border)"
		},
		"spacing": {
			"blockGap": "0rem",
			"padding": {
				"top": "0",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--30)",
				"right": "var(--wp--preset--spacing--30)"
			}
		},
		"color": {
			"text": "var(--wp--preset--color--contrast)",
			"background": "var(--wp--preset--color--base)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400",
			"lineHeight": "1.2"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"button": {
				"color": {
					"text": "var(--wp--preset--color--base)",
					"background": "var(--wp--preset--color--primary)"
				}
			}
		}
	},
	"customTemplates": [],
	"templateParts": [
		{
			"name": "header-default",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "header-front-page",
			"title": "Header on Front Page",
			"area": "header"
		},
		{
			"name": "footer-default",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "footer-front-page",
			"title": "Footer on Front Page",
			"area": "footer"
		},
		{
			"name": "loop-default",
			"title": "Posts Loop",
			"area": "loop"
		},
		{
			"name": "loop-slider",
			"title": "Posts Loop Slider",
			"area": "loop"
		}
	],
	"patterns": []
}
