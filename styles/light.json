{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Light Mode",
	"settings": {
		"color": {
			"palette": [
				{
					"name": "Base",
					"slug": "base",
					"color": "var(--wp--preset--color--white)"
				},
				{
					"name": "Base / Two",
					"slug": "base-2",
					"color": "var(--wp--custom--color--gray-900)"
				},
				{
					"name": "Contrast",
					"slug": "contrast",
					"color": "var(--wp--custom--color--gray-300)"
				},
				{
					"name": "Contrast / Two",
					"slug": "contrast-2",
					"color": "var(--wp--custom--color--gray-400)"
				},
				{
					"name": "Contrast / Three",
					"slug": "contrast-3",
					"color": "var(--wp--custom--color--gray-700)"
				},
				{
					"name": "Accent",
					"slug": "accent",
					"color": "var(--wp--custom--color--green-500)"
				},
				{
					"name": "Accent / Two",
					"slug": "accent-2",
					"color": "var(--wp--custom--color--green-700)"
				},
				{
					"name": "Accent / Three",
					"slug": "accent-3",
					"color": "var(--wp--custom--color--green-900)"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/calendar": {
				"color": {
					"text": "var(--wp--preset--color--contrast)",
					"background": "var(--wp--preset--color--base)"
				}
			}
		}
	}
}
